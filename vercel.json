{
  "framework": "create-react-app",
  "installCommand": "npm run install:all",
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "routes": [
    {
      "src": "/api/claude/messages",
      "methods": ["POST"],
      "dest": "/api/claude-messages"
    },
    {
      "src": "/api/dune/graphql",
      "methods": ["POST"],
      "dest": "/api/dune-graphql"
    },
    {
      "src": "/api/health",
      "methods": ["GET"],
      "dest": "/api/health"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
